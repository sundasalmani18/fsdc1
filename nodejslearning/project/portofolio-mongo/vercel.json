{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "./routes/portfolioRoutes.js",
      "use": "@vercel/node"
    },
    {
      "src": "./routes/supportRoutes.js",
      "use": "@vercel/node"
    }
  ],

  

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "app.js"
    },
    { "source": "/portfolio", "dest": "../controller/addData.js" },
    { "source": "/supportsystem", "dest": "../controller/supportController.js" }
  ]
}
  