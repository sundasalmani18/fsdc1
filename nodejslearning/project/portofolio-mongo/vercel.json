{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "../controller/addData.js",
      "use": "@vercel/node"
    },
    {
      "src": "../controller/supportController.js",
      "use": "@vercel/node"
    }
  ],

  

  "routes": [
    {
      "source": "/(.*)",
      "destination": "app.js"
    },
    { "src": "/portfolio", "dest": "../controller/addData.js" },
    { "src": "/supportsystem", "dest": "../controller/supportController.js" }
  ]
}
  